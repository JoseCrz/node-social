{
    "version": 2,
    "builds": [
        {
            "src": "api/app.js",
            "use": "@now/node"
        },
        {
            "src": "post/index-post.js",
            "use": "@now/node"
        }
    ],
    "routes": [
       {
           "src": "/api/auth(.*)",
           "dest": "/api/app.js"
       },
       {
           "src": "/api/user(.*)",
           "dest": "/api/app.js"
       },
       {
           "src": "/api/post(.*)",
           "dest": "/post/index-post.js"
       }
    ]
}